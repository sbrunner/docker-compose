{
  extends: [
    'github>camptocamp/gs-renovate-config-preset:base.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:group.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:ci.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:pre-commit.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:python.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:json-schema.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:shellcheck.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:preset.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:security.json5#1.1.1',
    'github>camptocamp/gs-renovate-config-preset:own.json5#1.1.1',
  ],
  customManagers: [
    {
      fileMatch: ['.*/.env$'],
      matchStrings: ['_TAG=(?<currentValue>[^\\s]+) +# (?<depName>[^\\s]+)'],
      datasourceTemplate: 'docker',
      customType: 'regex',
    },
  ],
  packageRules: [
    /** Automerge the patch, the minor and the dev dependency */
    {
      matchUpdateTypes: ['minor', 'patch'],
      automerge: true,
    },
    /** Group the patch and the minor */
    {
      matchUpdateTypes: ['patch'],
      groupName: 'all patch versions',
    },
    {
      matchUpdateTypes: ['minor'],
      groupName: 'all minor versions',
    },
  ],
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
}
